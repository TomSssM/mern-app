{
  "name": "mern-app",
  "private": true,
  "version": "1.0.0",
  "description": "Usual MERN Application",
  "main": "build/server.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack",
    "watch": "webpack --watch",
    "server": "nodemon --quiet --delay 1500ms",
    "start": "node ./build/server.js",
    "dev": "npm-run-all 'build:dev' --parallel watch server",
    "format": "prettier '.' '.*rc'",
    "lint": "npm-run-all 'lint:*'",
    "lint:eslint": "eslint --ext '.jsx,.js' .",
    "lint:format": "npm run format -- --check",
    "fix": "npm-run-all 'fix:*'",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:format": "npm run format -- --write"
  },
  "keywords": [
    "mern",
    "app",
    "react"
  ],
  "author": "TomSssM",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.27.1",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "core-js": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
